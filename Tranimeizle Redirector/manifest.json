{
  "manifest_version": 3,
  "name": "TrAnimeİzle Yönlendirici",
  "version": "1.0",
  "description": "www.tranimeizle.co adresini otomatik olarak www.tranimeizle.top adresine yönlendirir.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*.tranimeizle.co/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}